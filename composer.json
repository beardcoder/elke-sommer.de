{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "framework",
    "laravel"
  ],
  "require": {
    "php": "^8.3",
    "area17/twill": "^3",
    "area17/twill-image": "^2.0",
    "artesaos/seotools": "^1.3",
    "bepsvpt/secure-headers": "^8.0",
    "deployer/deployer": "^7.4",
    "guzzlehttp/guzzle": "^7.9",
    "hotwired-laravel/turbo-laravel": "^2.1",
    "jeroendesloovere/vcard": "^1.7",
    "laravel/framework": "^11.22",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.9",
    "ralphjsmit/laravel-seo": "^1.6",
    "spatie/laravel-honeypot": "^4.5",
    "spatie/laravel-responsecache": "^7.6",
    "spatie/laravel-sitemap": "^7.2",
    "spatie/laravel-varnish": "^2.10",
    "telegram-bot/api": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.13",
    "barryvdh/laravel-ide-helper": "^3.1",
    "driftingly/rector-laravel": "^1.2",
    "ergebnis/composer-normalize": "^2.43",
    "friendsofphp/php-cs-fixer": "^3.64",
    "laravel/sail": "^1.31",
    "nunomaduro/collision": "^8.4",
    "rector/rector": "^1.2",
    "spatie/laravel-ignition": "^2.8"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "ergebnis/composer-normalize": true
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "barryvdh/laravel-ide-helper"
      ]
    }
  },
  "scripts": {
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
